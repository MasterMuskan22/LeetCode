#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    bool canBeEqual(string s1, string s2) {
        vc odd1,even1,odd2,even2;
        rep(i,0,sz(s1)){
            if(i&1) odd1.pb(s1[i]);
            else even1.pb(s1[i]);
        }
        rep(i,0,sz(s2)){
            if(i&1) odd2.pb(s2[i]);
            else even2.pb(s2[i]);
        }
        srt(odd1);
        srt(even1);
        srt(odd2);
        srt(even2);
        return odd1==odd2 && even1==even2;
    }
};
