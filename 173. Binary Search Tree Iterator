#define speed ios_base::sync_with_stdio(false); cin.tie(NULL);
class BSTIterator {
public:
    //TC = O(1)
    //SC = O(H)
    stack<TreeNode*> st;
    BSTIterator(TreeNode* root) {
        //pushAll will push all the nodes that are to the left of root
        //We are basically following the Inorder Traversal only
        //Left Node Right
        //So if found any node push all right
        pushAll(root);
    }
    
    int next() {
        //Return the top of the stack as it will be the smaller one
        TreeNode* node=st.top();
        st.pop();
        //After node we have to go right and push all the left of that in stack
        pushAll(node->right);
        rt(node->val);
    }
    
    bool hasNext() {
        rt(!st.empty());
    }
private:
    void pushAll(TreeNode* root){
        for(;root!=NULL;){
            st.push(root);
            root=root->left;
        }
    }
};

/**
 * Your BSTIterator object will be instantiated and called as such:
 * BSTIterator* obj = new BSTIterator(root);
 * int param_1 = obj->next();
 * bool param_2 = obj->hasNext();
 */
