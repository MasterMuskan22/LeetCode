int mod=1e9+7;
#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        int prod=1;
        int cnt=0;
        for(auto it:nums){
            if(it==0) cnt++;
            else prod*=it;
        }
        vi res;
        rep(i,0,sz(nums)){
            if(cnt==0) res.pb(prod/nums[i]);
            else if(cnt==1){
                if(nums[i]!=0) res.pb(0);
                else res.pb(prod);
            }
            else res.pb(0);
        }
        rt(res);
    }
};
