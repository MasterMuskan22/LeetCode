#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    bool asteroidsDestroyed(int mass, vector<int>& asteroids) {
        ll sum=0;
        for(auto it:asteroids){
            if(it<=mass) sum+=it;
        }
        srt(asteroids);
        int ind=-1;
        rep(i,0,sz(asteroids)){
            if(asteroids[i]<=mass) ind=i;
            else break;
        }
        sum+=mass;
        bool ch=1;
        rep(i,ind+1,sz(asteroids)){
            if(sum>=asteroids[i]){
                sum+=asteroids[i];
                ch=1;
            }
            else{
                ch=0;
                break;
            }
        }
        return ch==1?1:0;
    }
};
