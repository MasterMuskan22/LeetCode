#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    vector<vector<int>> findPrimePairs(int n) {
        vector<bool> sieve(1e6+1,1);
        sieve[0]=0,sieve[1]=0;
        for(int i=2;i*i<=n;i++){
            if(sieve[i]==1){
                for(int j=i*i;j<=n;j+=i) sieve[j]=0;
            }
        }
        vii res;
        rep(i,2,n/2+1){
            if(sieve[i]==1 && sieve[n-i]==1) res.push_back({i,n-i});
        }
        rt(res);
    }
};
