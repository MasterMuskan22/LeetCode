#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    int numOfSubarrays(vector<int>& arr, int k, int threshold) {
        int cnt=0;
        int cur_sum=0;
        int i=0,j=0;
        while(j<sz(arr)){
            while(j-i<k) cur_sum+=arr[j++];
            if(cur_sum/k>=threshold) cnt++;
            cur_sum-=arr[i++];
        }
        rt(cnt);
    }
};
