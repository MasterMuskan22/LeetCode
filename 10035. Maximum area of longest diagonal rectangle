#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    int areaOfMaxDiagonal(vector<vector<int>>& dimensions) {
        int max_area=0;
        double diag_len=0.0;
        for(auto it:dimensions){
            double cur=(double)(sqrt((ll)it[0]*it[0]+(ll)it[1]*it[1]));
            if(cur>=diag_len){
                if(cur==diag_len) max_area=max(max_area,it[0]*it[1]);
                else max_area=it[0]*it[1];
                diag_len=cur;
            }
        }
        rt(max_area);
    }
};
