#define speed ios_base::sync_with_stdio(false); cin.tie(NULL);
class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        int n=sz(nums);
        if(k>n){
            int x=k/n;
            k-=x*n;
        }
        n-=k;
        reverse(nums.begin(),nums.begin()+n);
        reverse(nums.begin()+n,nums.end());
        rev(nums);
    }
};
