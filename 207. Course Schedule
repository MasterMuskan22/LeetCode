int mod=1e9+7;
#define speed ios_base::sync_with_stdio(false); cin.tie(NULL);
class Solution {
public:
    bool dfs(int node,vi adjList[],vi &vis,vi &pathVis){
        vis[node]=1;
        pathVis[node]=1;
        for(auto it:adjList[node]){
            if(!vis[it]){
                if(dfs(it,adjList,vis,pathVis)==1) rt(1);
            }
            else if(pathVis[it]==1) rt(1);
        }
        pathVis[node]=0;
        rt(0);
    }
    bool canFinish(int numCourses, vector<vector<int>>& prerequisites) {
        vi adjList[numCourses];
        for(auto it:prerequisites) adjList[it[1]].pb(it[0]);
        vi vis(numCourses,0);
        vi pathVis(numCourses,0);
        rep(i,0,numCourses){
            if(!vis[i]){
                if(dfs(i,adjList,vis,pathVis)==1) rt(0);
            }
        }
        rt(1);
        // queue<int> q;
        // vi adjList[numCourses];
        // for(auto it:prerequisites) adjList[it[1]].pb(it[0]);
        // vi ind(numCourses,0);
        // rep(i,0,numCourses){
        //     for(auto it:adjList[i]) ind[it]++;
        // }
        // rep(i,0,numCourses){
        //     if(ind[i]==0) q.push(i);
        // }
        // int cnt=0;
        // while(!q.empty()){
        //     int node=q.front();
        //     q.pop();
        //     cnt++;
        //     for(auto it:adjList[node]){
        //         ind[it]--;
        //         if(ind[it]==0) q.push(it);
        //     }
        // }
        // rt(cnt==numCourses);
    }
};
