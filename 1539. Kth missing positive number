#define speed ios_base::sync_with_stdio(false); cin.tie(NULL);
class Solution {
public:
    int findKthPositive(vector<int>& arr, int k) {
        int start=0,end=sz(arr)-1;
        while(start<=end){
            int mid=start+(end-start)/2;
            if(arr[mid]-mid-1<k) start=mid+1;
            else end=mid-1;
        }
        rt(start+k);
    }
};
