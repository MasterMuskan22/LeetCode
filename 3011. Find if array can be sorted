#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    bool canSortArray(vector<int>& nums) {
        int n=sz(nums);
        rep(j,0,100){
            rep(i,0,n-1){
                if(nums[i]>nums[i+1]){
                    int cnt1=__builtin_popcount(nums[i]);
                    int cnt2=__builtin_popcount(nums[i+1]);
                    if(cnt1==cnt2) swap(nums[i],nums[i+1]);
                }
            }
        }
        rep(i,1,n){
            if(nums[i]<nums[i-1]) rt(0);
        }
        rt(1);
    }
};
