int mod=1e9+7;
#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    string removeKdigits(string num, int k) {
        int n=sz(num);
        stack<char> st;
        st.push(num[0]);
        rep(i,1,n){
            while(!st.empty() && k>0 && (st.top())>(num[i])){
                k--;
                st.pop();
            }
            st.push(num[i]);
        }
        while(k-- && !st.empty()){
            st.pop();
        }
        string res="";
        while(!st.empty()){
            res+=st.top();
            st.pop();
        }
        rev(res);
        int i=0;
        while(i<sz(res) && res[i]=='0') i++;
        res=res.substr(i);
        if(sz(res)==0) rt("0");
        return i==n?"0":res;
    }
};
