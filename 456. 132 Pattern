#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    bool find132pattern(vector<int>& nums) {
        int n=sz(nums);
        vi prevMin(n,0);
        prevMin[0]=nums[0];
        rep(i,1,n) prevMin[i]=min(prevMin[i-1],nums[i]);
        stack<int> st;
        for(int i=n-1;i>=0;i--){
            if(nums[i]>prevMin[i]){
                while(!st.empty() && st.top()<=prevMin[i]) st.pop();
                if(!st.empty() && st.top()<nums[i]) rt(1);
                st.push(nums[i]); 
            }
        }
        rt(0);
    }
};
