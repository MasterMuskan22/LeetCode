#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
bool isSorted(vi &arr){rep(i,1,sz(arr)){if(arr[i]<=arr[i-1]) rt(0);}rt(1);}
class Solution {
public:
    int incremovableSubarrayCount(vector<int>& nums) {
        int cnt=0;
        int n=sz(nums);
        rep(i,0,n){
            rep(j,i,n){
                vi v;
                rep(k,0,i) v.pb(nums[k]);
                rep(k,j+1,n) v.pb(nums[k]);
                if(isSorted(v)) cnt++;
            }
        }
        rt(cnt);
    }
};
