int mod=1e9+7;
#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    vector<vector<int>> levelOrder(Node* root) {
        if(!root) rt({});
        vii res;
        queue<Node*> q;
        q.push(root);
        while(!q.empty()){
            int n=sz(q);
            vi temp;
            while(n--){
                auto it=q.front();
                q.pop();
                temp.pb(it->val);
                for(auto node:it->children) q.push(node);
            }
            res.pb(temp);
        }
        rt(res);
    }
};
