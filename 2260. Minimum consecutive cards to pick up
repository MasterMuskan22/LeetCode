#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    int minimumCardPickup(vector<int>& nums) {
        unordered_map<int,vi> mp;
        rep(i,0,sz(nums)) mp[nums[i]].pb(i);
        int min_cards=1e9;
        for(auto it:mp){
            if(sz(it.second)>1){
                rep(i,1,sz(it.second)) min_cards=min(min_cards,it.second[i]-it.second[i-1]);
            }
        }
        return min_cards==1e9?-1:min_cards+1;
    }
};
