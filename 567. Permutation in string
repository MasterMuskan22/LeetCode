int dc1[8]={0,1,1,1,0,-1,-1,-1};
int mod=1e9+7;
#define speed ios_base::sync_with_stdio(false); cin.tie(NULL); cout.precision(20);
class Solution {
public:
    bool checkInclusion(string s1, string s2) {
        vi mp(26,0);
        for(auto it:s1) mp[it-'a']++;
        int i=0,j=0;
        vi cur(26,0);
        while(j<sz(s2)){
            cur[s2[j]-'a']++;
            if(j-i+1==sz(s1)){
                if(cur==mp) rt(1);
            }
            if(j-i+1>=sz(s1)){
                cur[s2[i]-'a']--;
                i++,j++;
            }
            else j++;
        }
        rt(0);
    }
};
